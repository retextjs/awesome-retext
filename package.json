{
  "name": "awesome-retext",
  "private": true,
  "version": "0.0.0",
  "description": "Curated list of awesome retext resources",
  "license": "CC-BY-4.0",
  "keywords": [],
  "repository": "retextjs/awesome-retext",
  "bugs": "https://github.com/retextjs/awesome-retext/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "devDependencies": {
    "remark-cli": "^7.0.0",
    "remark-preset-wooorm": "^6.0.0"
  },
  "scripts": {
    "format": "remark . -qo",
    "test": "npm run format"
  },
  "remarkConfig": {
    "settings": {
      "listItemIndent": "1"
    },
    "plugins": [
      "preset-wooorm",
      [
        "lint-list-item-indent",
        "space"
      ],
      [
        "toc",
        {
          "heading": "contents"
        }
      ]
    ]
  }
}
